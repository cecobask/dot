#!/bin/bash
#Generated by JetBrains Toolbox 2.5.1.34629 at 2024-11-15T10:01:56

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/tdimov/Applications/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${intellij_args[@]}"
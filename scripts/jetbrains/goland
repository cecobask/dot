#!/bin/bash
#Generated by JetBrains Toolbox 2.4.2.32922 at 2024-10-22T10:40:52.468469

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/tdimov/Applications/GoLand.app/Contents/MacOS/goland" $wait --args "${intellij_args[@]}"